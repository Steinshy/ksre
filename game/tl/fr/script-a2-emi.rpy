    # emi "W-what? Oh, yeah, I guess he is."
    emi "Q-quoi? Oh, ouais, c'est vrai."

# game/script-a2-emi.rpy:5907
translate fr a2_emi_famous_last_words_b1f2d67d:

    # emi "This is my friend Hisao."
    emi "C'est Hisao, un ami."

# game/script-a2-emi.rpy:5909
translate fr a2_emi_famous_last_words_c4f4d256:

    # hi "We've met."
    hi "On s'est rencontrés."

# game/script-a2-emi.rpy:5914
translate fr a2_emi_famous_last_words_8e8aecc6:

